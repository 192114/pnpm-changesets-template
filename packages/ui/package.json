{
  "name": "@infras/ui",
  "version": "1.0.0",
  "description": "An infrastructure monorepo ui library",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
